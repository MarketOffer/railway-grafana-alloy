{
  "id": null,
  "uid": "platform-overview",
  "title": "Platform Overview",
  "tags": [
    "generated",
    "marketoffer"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "label": "Service",
        "type": "custom",
        "query": "marketoffer-services,worker-company-discovery,worker-director-discovery,worker-owner-discovery,worker-person-linkedin,worker-site-fetch,worker-site-fetchanalysis,worker-site-fetchanalysis-2,worker-site-fetchfetcher,worker-site-fetchfetcher-2",
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "options": []
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Up Ratio (avg across selected)",
      "datasource": null,
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "avg(up{service=~\"$service\"}) * 100",
          "legendFormat": "up %",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [ "lastNotNull" ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 80 },
              { "color": "green", "value": 95 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "table",
      "title": "Up Status by Service",
      "datasource": null,
      "gridPos": { "h": 8, "w": 16, "x": 8, "y": 0 },
      "targets": [
        {
          "expr": "up{service=~\"$service\"}",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "instance": true,
              "job": true,
              "metric": true
            },
            "indexByName": {}
          }
        }
      ],
      "options": {
        "showHeader": true,
        "footer": {
          "show": false
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Scrape Duration p95 (5m)",
      "datasource": null,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (service, le) (rate(prometheus_scrape_duration_seconds_bucket{service=~\"$service\"}[5m])))",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": []
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      }
    },
    {
      "id": 4,
      "type": "bargauge",
      "title": "Scrape Samples (last scrape)",
      "datasource": null,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "scrape_samples_scraped{service=~\"$service\"}",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 200 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal"
      }
    }
  ]
}

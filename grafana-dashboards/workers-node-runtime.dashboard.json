        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "options": []
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Up",
      "datasource": null,
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "up{service=~\"$service\"}",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "reduceOptions": {
          "calcs": [ "lastNotNull" ],
          "fields": "",
          "values": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Scrape Duration p95 (5m)",
      "datasource": null,
      "gridPos": { "h": 8, "w": 18, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (service, le) (rate(prometheus_scrape_duration_seconds_bucket{service=~\"$service\"}[5m])))",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": []
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"

{
  "id": null,
  "uid": "workers-queue-health",
  "title": "Workers - Queue Health (Scrape/Sends)",
  "tags": [
    "generated",
    "workers"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "label": "Service",
        "type": "custom",
        "query": "marketoffer-services,worker-company-discovery,worker-director-discovery,worker-owner-discovery,worker-person-linkedin,worker-site-fetch,worker-site-fetchanalysis,worker-site-fetchanalysis-2,worker-site-fetchfetcher,worker-site-fetchfetcher-2",
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "options": []
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Up (selected services)",
      "datasource": null,
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "avg(up{service=~\"$service\"})",
          "legendFormat": "avg up",
          "refId": "A"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "reduceOptions": {
          "calcs": [ "lastNotNull" ],
          "fields": "",
          "values": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Samples Delivered to Grafana Cloud (5m rate)",
      "datasource": null,
      "gridPos": { "h": 8, "w": 18, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "sum by (service) (rate(prometheus_remote_storage_samples_total{service=~\"$service\"}[5m]))",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": []
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 100 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Scrape Success Rate (5m)",
      "datasource": null,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "sum by (service) (rate(scrape_duration_seconds_count{service=~\"$service\"}[5m]))",
          "legendFormat": "{{service}} scrapes",
          "refId": "A"
        },
        {
          "expr": "sum by (service) (rate(scrape_duration_seconds_count{service=~\"$service\", scrape_error=\"true\"}[5m]))",
          "legendFormat": "{{service}} scrape errors",
          "refId": "B"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": []
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "type": "bargauge",
      "title": "Samples Scraped (latest)",
      "datasource": null,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "scrape_samples_scraped{service=~\"$service\"}",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 200 }
            ]
          }
        },
        "overrides": []
      }
    }
  ]
}
